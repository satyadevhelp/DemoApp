<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="ProfileHandler.Views.MainTabbedPage"
            xmlns:view="clr-namespace:ProfileHandler.Views"
            NavigationPage.HasNavigationBar="False">
    <NavigationPage Title="Add Profile" Visual="Material">
        <x:Arguments>
            <view:AddProfilePage />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Profile List" Visual="Material">
        <x:Arguments>
            <view:ProfileListPage />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>